#include <stdio.h>
int main() {
    int i,n;
    for(n=1;n<=5;n++)
    {
        for(i=1;i<=n;i++)
        {
         printf("%d",i);
        }
      i=i-1;
      printf("\n");
    }
      i=4;
      for(n=4;n>=1;n--)
      {
      for(i=1;i<=n;i++)
      {
          printf ("%d",i);
      }
        printf("\n");
    }
    return 0;
}

Output:
1
12
123
1234
12345
1234
123
12
1
