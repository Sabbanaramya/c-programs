#include <stdio.h>
void main() {
  int n,rev= 0, r,temp=0;
    scanf("%d", &n);
    temp = n;
    while (n != 0) 
    {
        r = n % 10;
        rev =rev * 10 + r;
        n=n/10;
    }
    if (temp == rev)
 printf("%d is a palindrome",rev);
    else
 printf("%d is not a palindrome",rev);     
}


Output :
12321
12321 is a palindrome

