#include <stdio.h> 
void main()
 {
  int n, i, p = 0;
  scanf("%d", &n);
  for (i = 1; i <= n; i++) 
{
      if (n % i == 0)
 {
         P++;
      }
  }

  if (p == 2) 
{
        printf("n is a Prime number");
  }
  else
 {
         printf("n is not a Prime number");
  } 
}


Output: 
