#include <stdio.h>
void main() {
  int n,sum= 0, r,temp=0;
    scanf("%d", &n);
    temp = n;
    while (n != 0) 
    {
        r = n % 10;
        n=n/10;
        sum=sum+(r*r*r);
}
    if (temp == sum)
 printf("%d is a Armstrong",temp);
    else
 printf("%d is not an Armstrong",temp);
}
Output:
153
153 is a Armstrong
