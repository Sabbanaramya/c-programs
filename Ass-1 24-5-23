# include<stdio.h>
void main()
{
int arr1[3],arr2[3],arr3[6],i,j=0;
printf("enter elements of array 1:');
for(i=0;i<3;i++)
{
scanf("%d",&arr1[i]);
}
printf("enter elements of array 2:");
for(i=0;i<3;i++)
{
scanf("%d",&arr2);
}
for(i=0;i<3;i++)
{
arr3[j]= arr1[i];
j++;
}
for(i=0;i<3;i++)
{
arr3[j]=arr2[i];
j++;
}
printf("\n elements of array 3:");
for(i=0;i<6;i++)
{
printf("%d",arr3[i]);
}
}

output:
enter elements of array 1:
1
2
3
enter elements of array 2:
10
20
30
elements of array 3:
1 2 3 10 20 30
